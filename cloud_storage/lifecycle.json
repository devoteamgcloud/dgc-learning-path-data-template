{
    "lifecycle": {
        "rule": [
            {
                "action": {
                    "type": "SetStorageClass",
                    "storageClass": "NEARLINE"
                },
                "condition": {
                    "age": 30,
                    "matchesPrefix": [
                        "archive/"
                    ]
                }
            },
            {
                "action": {
                    "type": "SetStorageClass",
                    "storageClass": "COLDLINE"
                },
                "condition": {
                    "age": 90,
                    "matchesPrefix": [
                        "archive/"
                    ]
                }
            },
            {
                "action": {
                    "type": "SetStorageClass",
                    "storageClass": "ARCHIVE"
                },
                "condition": {
                    "age": 365,
                    "matchesPrefix": [
                        "archive/"
                    ]
                }
            },
            {
                "action": {
                    "type": "Delete"
                },
                "condition": {
                    "age": 1825,
                    "matchesPrefix": [
                        "archive/"
                    ]
                }
            }
        ]
    }
}